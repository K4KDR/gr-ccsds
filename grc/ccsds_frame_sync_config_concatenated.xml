<?xml version="1.0"?>
<block>
	<name>Frame Sync. Config (Concatenated)</name>
	<key>variable_frame_sync_config_concatenated</key>
	<category>[ccsds]</category>
	<import>import ccsds</import>
	<var_make>
self.$(id) = $id = ccsds.frame_sync_config(ccsds.constellation_ccsds_$(modulation)().base(),
    '81C971AA73D3E0',
    52,
    $interleaving_depth*255*8*2+12,
    0,
    0
)
	</var_make>
	<var_value>ccsds.frame_sync_config(ccsds.constellation_ccsds_$(modulation)().base(),'81C971AA73D3E0',52,$interleaving_depth*255*8*2+12,0,0)</var_value>
	<make></make>
	<param>
		<name>Modulation</name>
		<key>modulation</key>
		<type>enum</type>
		<option>
			<name>BPSK</name>
			<key>bpsk</key>
		</option>
		<option>
			<name>QPSK</name>
			<key>qpsk</key>
		</option>
	</param>
	<param>
		<name>Interleaving depth</name>
		<key>interleaving_depth</key>
		<value>4</value>
		<type>int</type>
	</param>
</block>
