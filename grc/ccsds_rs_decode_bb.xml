<?xml version="1.0"?>
<block>
  <name>Reed Solomon Decoder (CCSDS)</name>
  <key>ccsds_rs_decode_bb</key>
  <category>CCSDS/Channelcoding</category>
  <import>import ccsds</import>
  <make>ccsds.rs_decode_bb($(id)_msgq_out, $codeword_length)</make>
  <param>
    <name>Codeword length</name>
    <key>codeword_length</key>
    <value>10200</value>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>msg</type>
  </source>
</block>
