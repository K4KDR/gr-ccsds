<?xml version="1.0"?>
<block>
  <name>Frame Synchronization (symb)</name>
  <key>ccsds_frame_sync</key>
  <category>[ccsds]</category>
  <import>import ccsds</import>
  <make>ccsds.frame_sync($sync_config, $gen_config, $additional_frames)</make>
  <param>
		<name>Frame synchronization config</name>
		<key>sync_config</key>
		<value></value>
		<type>raw</type>
	</param>
	<param>
		<name>Frame generation config</name>
		<key>gen_config</key>
		<value></value>
		<type>raw</type>
	</param>
	<param>
		<name>Additional ASM positions</name>
		<key>additional_frames</key>
		<value>ccsds.asm_position_list()</value>
		<type>raw</type>
	</param>
	<sink>
		<name>in</name>
		<type>complex</type>
	</sink>

	<source>
		<name>out</name>
		<type>message</type>
	</source>
</block>
