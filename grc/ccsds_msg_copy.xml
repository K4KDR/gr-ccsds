<?xml version="1.0"?>
<block>
  <name>Message Copy (CCSDS)</name>
  <key>ccsds_msg_copy</key>
  <category>CCSDS</category>
  <import>import ccsds</import>
  <make>ccsds.msg_copy()</make>
	<sink>
		<name>in</name>
		<type>message</type>
	</sink>
	<source>
		<name>out</name>
		<type>message</type>
	</source>
	<doc>Copies all messages from in top out port.</doc>
</block>
